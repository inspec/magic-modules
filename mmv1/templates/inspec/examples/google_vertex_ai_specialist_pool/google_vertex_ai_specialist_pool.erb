<% gcp_project_id = "#{external_attribute(pwd, 'gcp_project_id', doc_generation)}" -%>
<% specialist_pool = grab_attributes(pwd)['specialist_pool'] -%>
describe google_vertex_ai_specialist_pool(name: "projects/#{gcp_project_id}/locations/#{specialist_pool['region']}/specialistPools/#{specialist_pool['name']}", region: <%= doc_generation ? "' #{specialist_pool['region']}'":"specialist_pool['region']" -%>) do
	it { should exist }
	its('display_name') { should cmp <%= doc_generation ? "'#{specialist_pool['display_name']}'" : "specialist_pool['display_name']" -%> }
	its('name') { should cmp <%= doc_generation ? "'#{specialist_pool['name']}'" : "specialist_pool['name']" -%> }

end

describe google_vertex_ai_specialist_pool(name: "does_not_exit", region: <%= doc_generation ? "' #{specialist_pool['region']}'":"specialist_pool['region']" -%>) do
	it { should_not exist }
end