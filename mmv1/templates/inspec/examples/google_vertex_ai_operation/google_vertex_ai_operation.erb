<% gcp_project_id = "#{external_attribute(pwd, 'gcp_project_id', doc_generation)}" -%>
<% operation = grab_attributes(pwd)['operation'] -%>
describe google_vertex_ai_operation(name: "projects/#{gcp_project_id}/locations/#{operation['region']}/operations/#{operation['name']}", region: <%= doc_generation ? "' #{operation['region']}'":"operation['region']" -%>) do
	it { should exist }
	its('name') { should cmp <%= doc_generation ? "'#{operation['name']}'" : "operation['name']" -%> }

end

describe google_vertex_ai_operation(name: "does_not_exit", region: <%= doc_generation ? "' #{operation['region']}'":"operation['region']" -%>) do
	it { should_not exist }
end