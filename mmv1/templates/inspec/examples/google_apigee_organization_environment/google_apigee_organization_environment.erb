	its('last_modified_at') { should cmp <%= doc_generation ? "'#{organization_environment['last_modified_at']}'" : "organization_environment['last_modified_at']" -%> }
	its('display_name') { should cmp <%= doc_generation ? "'#{organization_environment['display_name']}'" : "organization_environment['display_name']" -%> }
	its('name') { should cmp <%= doc_generation ? "'#{organization_environment['name']}'" : "organization_environment['name']" -%> }
	its('description') { should cmp <%= doc_generation ? "'#{organization_environment['description']}'" : "organization_environment['description']" -%> }
	its('created_at') { should cmp <%= doc_generation ? "'#{organization_environment['created_at']}'" : "organization_environment['created_at']" -%> }

end

