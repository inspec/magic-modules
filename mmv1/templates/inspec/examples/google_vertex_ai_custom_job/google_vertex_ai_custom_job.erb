<% gcp_project_id = "#{external_attribute(pwd, 'gcp_project_id', doc_generation)}" -%>
<% custom_job = grab_attributes(pwd)['custom_job'] -%>

describe google_vertex_ai_custom_job(name: "projects/#{<%= gcp_project_id -%>}/locations/#{<%= doc_generation ? "'#{custom_job['region']}'" : "custom_job['region']" -%>}/customJobs/#{<%= doc_generation ? "'#{custom_job['job_id']}'" : "custom_job['job_id']" -%>}", region: <%= doc_generation ? "' #{custom_job['region']}'":"custom_job['region']" -%>) do
  it { should exist }
  its('display_name') { should cmp custom_job['name'] }
end
