<% gcp_project_id = "#{external_attribute(pwd, 'gcp_project_id', doc_generation)}" -%>
  <% region_autoscaler = grab_attributes(pwd)['region_autoscaler'] -%>
  describe google_compute_region_autoscalers(project: <%= gcp_project_id -%>, region: <%= doc_generation ? "' #{region_autoscaler['region']}'":"region_autoscaler['region']" -%>) do
    it { should exist }
  end