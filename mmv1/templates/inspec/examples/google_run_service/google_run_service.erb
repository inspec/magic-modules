<% gcp_project_id = "#{external_attribute(pwd, 'gcp_project_id', doc_generation)}" -%>
<% service = grab_attributes(pwd)['service'] -%>
describe google_run_service(name: <%= doc_generation ? "' #{service['name']}'":"service['name']" -%>) do
	it { should exist }
	its('api_version') { should cmp <%= doc_generation ? "'#{service['api_version']}'" : "service['api_version']" -%> }
	its('kind') { should cmp <%= doc_generation ? "'#{service['kind']}'" : "service['kind']" -%> }

end

describe google_run_service(name: "does_not_exit") do
	it { should_not exist }
end