<% gcp_project_id = "#{external_attribute(pwd, 'gcp_project_id', doc_generation)}" -%>
  <% vpn_gateway = grab_attributes(pwd)['vpn_gateway'] -%>
  describe google_compute_vpn_gateways(project: <%= gcp_project_id -%>, region: <%= doc_generation ? "' #{vpn_gateway['region']}'":"vpn_gateway['region']" -%>) do
    it { should exist }
  end