<% gcp_project_id = "#{external_attribute(pwd, 'gcp_project_id', doc_generation)}" -%>
  <% datasets_data_item = grab_attributes(pwd)['datasets_data_item'] -%>
  describe google_vertex_ai_datasets_data_items(parent: "projects/#{gcp_project_id}/locations/#{datasets_data_item['region']}/datasets/#{datasets_data_item['dataset']}", region: <%= doc_generation ? "' #{datasets_data_item['region']}'":"datasets_data_item['region']" -%>) do
    it { should exist }
  end