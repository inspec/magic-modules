<% gcp_project_id = "#{external_attribute(pwd, 'gcp_project_id', doc_generation)}" -%>
   <% training_pipeline = grab_attributes(pwd)['training_pipeline'] -%>


   describe google_vertex_ai_training_pipeline(name: "projects/#{gcp_project_id}/locations/#{training_pipeline['region']}/trainingPipelines/#{training_pipeline['job_id']}", region: <%= doc_generation ? "' #{training_pipeline['region']}'":"training_pipeline['region']" -%>) do
     it { should exist }
   end
   describe google_vertex_ai_training_pipeline(name: "does_not_exit", region: <%= doc_generation ? "' #{training_pipeline['region']}'":"training_pipeline['region']" -%>) do
     it { should_not exist }
   end
