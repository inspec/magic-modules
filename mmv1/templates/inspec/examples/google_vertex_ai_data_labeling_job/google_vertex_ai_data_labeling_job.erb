<% gcp_project_id = "#{external_attribute(pwd, 'gcp_project_id', doc_generation)}" -%>
<% data_labeling_job = grab_attributes(pwd)['data_labeling_job'] -%>
describe google_vertex_ai_data_labeling_job(name: "projects/#{gcp_project_id}/locations/#{data_labeling_job['region']}/dataLabelingJobs/#{data_labeling_job['name']}", region: <%= doc_generation ? "' #{data_labeling_job['region']}'":"data_labeling_job['region']" -%>) do
it { should exist }
	its('create_time') { should cmp <%= doc_generation ? "'#{data_labeling_job['create_time']}'" : "data_labeling_job['create_time']" -%> }
	its('inputs_schema_uri') { should cmp <%= doc_generation ? "'#{data_labeling_job['inputs_schema_uri']}'" : "data_labeling_job['inputs_schema_uri']" -%> }
	its('instruction_uri') { should cmp <%= doc_generation ? "'#{data_labeling_job['instruction_uri']}'" : "data_labeling_job['instruction_uri']" -%> }
	its('update_time') { should cmp <%= doc_generation ? "'#{data_labeling_job['update_time']}'" : "data_labeling_job['update_time']" -%> }
	its('name') { should cmp <%= doc_generation ? "'#{data_labeling_job['name']}'" : "data_labeling_job['name']" -%> }
	its('display_name') { should cmp <%= doc_generation ? "'#{data_labeling_job['display_name']}'" : "data_labeling_job['display_name']" -%> }
	its('state') { should cmp <%= doc_generation ? "'#{data_labeling_job['state']}'" : "data_labeling_job['state']" -%> }

end

describe google_vertex_ai_data_labeling_job(name: "does_not_exit", region: <%= doc_generation ? "' #{data_labeling_job['region']}'":"data_labeling_job['region']" -%>) do
	it { should_not exist }
end